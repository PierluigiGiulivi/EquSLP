data <- rpois(n=150,20)
poisson_loglikelihood <- function(lambda,x)
{
n    <-length(x)                # n is the sample size
llik <- -n*lambda+sum(x*log(lambda))-sum(log(factorial(x)))
return(llik)
}
maximisation <- optimize(poisson_loglikelihood,
interval=c(1,50),x=data,maximum=TRUE)
